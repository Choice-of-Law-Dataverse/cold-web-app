{
  "table_name": "Arbitral Institutions",
  "description": "Pretty mapping for Arbitral Institutions with related awards, rules, provisions, and jurisdictions",
  "version": "1.0",
  "mappings": {
    "direct_mappings": {
      "source_table": "source_table",
      "id": "CoLD_ID",
      "rank": "rank",
      "ID": "CoLD_ID",
      "Record ID": "ncRecordId",
      "Institution": "Institution",
      "Abbreviation": "Abbreviation",
      "Created": "Created",
      "Last Modified": "updated_at",
      "Last Modified By.id": "updated_by",
      "Created By.id": "created_by"
    },
    "nested_mappings": {
      "related_arbitral_awards": {
        "source_array": "related_arbitral_awards",
        "array_operations": {
          "Arbitral Awards": {"operation": "join", "field": "Case_Number", "separator": ", "},
          "Arbitral Awards Link": {"operation": "join", "field": "ncRecordId", "separator": ","}
        }
      },
      "related_arbitral_rules": {
        "source_array": "related_arbitral_rules",
        "array_operations": {
          "Arbitral Rules": {"operation": "join", "field": "Set_of_Rules", "separator": ", "},
          "Arbitral Rules In Force From": {"operation": "join", "field": "In_Force_From", "separator": ", "},
          "Arbitral Rules Link": {"operation": "join", "field": "ncRecordId", "separator": ","}
        }
      },
      "related_arbitral_provisions": {
        "source_array": "related_arbitral_provisions",
        "array_operations": {
          "Arbitral Provisions (Articles)": {"operation": "join", "field": "Article", "separator": ", "},
          "Arbitral Provisions Link": {"operation": "join", "field": "ncRecordId", "separator": ","}
        }
      },
      "related_jurisdictions": {
        "source_array": "related_jurisdictions",
        "array_operations": {
          "Jurisdictions": {"operation": "join", "field": "Name", "separator": ", "},
          "Jurisdictions Alpha-3 Code": {"operation": "join", "field": "Alpha_3_Code", "separator": ","},
          "Jurisdictions Link": {"operation": "join", "field": "ncRecordId", "separator": ","}
        }
      }
    }
  },
  "post_processing": {
    "remove_null_values": true,
    "remove_empty_strings": false
  }
}
