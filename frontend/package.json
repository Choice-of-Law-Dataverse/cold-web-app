{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^22.x"
  },
  "scripts": {
    "build": "nuxt build --log-level silent",
    "check": "npm run format && npm run lint:fix && npm run typecheck && npm run test:run",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxt start",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix --quiet",
    "format": "prettier --log-level silent --write .",
    "format:check": "prettier --log-level silent --check .",
    "knip": "knip",
    "typecheck": "vue-tsc --noEmit --pretty false -p tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --reporter=dot",
    "test:coverage": "vitest run --reporter=dot --coverage"
  },
  "dependencies": {
    "@auth0/auth0-nuxt": "^1.0.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@nuxt/content": "^3.11.0",
    "@nuxt/fonts": "^0.13.0",
    "@nuxt/icon": "^2.2.1",
    "@nuxt/scripts": "^0.13.2",
    "@nuxt/ui": "^4.4.0",
    "@nuxtjs/leaflet": "^1.3.2",
    "@nuxtjs/robots": "^5.6.7",
    "@nuxtjs/sitemap": "^7.5.2",
    "@opentelemetry/api": "^1.9.0",
    "@pydantic/logfire-node": "^0.11.6",
    "@tanstack/vue-query": "^5.92.9",
    "@tanstack/vue-query-devtools": "^5.91.0",
    "@unhead/vue": "^2.1.2",
    "better-sqlite3": "^12.6.2",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "mitt": "^3.0.1",
    "nuxt": "^4.3.0",
    "v-calendar": "^3.1.2",
    "vue": "^3.5.21",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.2.53",
    "@nuxt/eslint": "^1.13.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.19.7",
    "@vitest/ui": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "baseline-browser-mapping": "^2.9.18",
    "eslint": "^9.39.2",
    "eslint-plugin-perfectionist": "^4.15.1",
    "happy-dom": "^15.11.7",
    "knip": "^5.82.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.3"
  }
}
